---
import '../styles/navbar.css'
import {Image} from 'astro:assets';
import myImageFooter from "../assets/logo-plankton.webp";

---

<div class="content" id="miDiv">
  <div class="head-grid">

    <div class="head-logo">
      <a href="/"><Image src={myImageFooter} alt="Plankton-News-Logo" class="header-img"/>
      </a>
    </div>

    <div class="nav-links">
      <div class="elementos-menu">

    
      <a href="/">Inicio</a>
      <a href="/servicios/">Servicios</a>
      <a href="/blog/">Blog</a>
      <a href="/contacto/">Contacto</a>
      <a href="https://plankton.news/">News</a>
      <a href="https://planktonwallet.com/" class="ingresar-btn">Ingresar</a>

      
    </div>  

    <div class="socialmedia-navlinks">
      <i class="bi bi-facebook"></i>
      <i class="bi bi-twitter-x"></i>
      <i class="bi bi-instagram"></i>
      <i class="bi bi-linkedin"></i>
    </div>


      <div class="nav-links-grid">
        
        <div class="navlinks-logo">
          <a href="/"><Image src={myImageFooter} alt="Plankton-News-Logo" class="navlinks-img"/>
          </a>
        </div>

        <div class="close">
          <i class="bi bi-x-lg"></i>
      
        </div>

      </div>
  
    </div> 


    <div class="hamburger">
      <span class="line"></span>
      <span class="line"></span>
      <span class="line"></span>
    </div>

  </div>
</div>

